{
    "staticgen": {
        "db_file": "./.pydgeot.db",
        "force_generate": false
    },
    "devserver": {
        "address": "localhost",
        "port": 8080,
        "index_files!": [
            "index.html",
            "index.htm"
        ],
        "redirects": {
            "/url/": "/var/www/some/thing/"
        }
    },
    "ignore": {
        "dot_files": true,
        "paths": [
            "^\\.git(/.*)?$",
            "^\\.templates(/.*)?$"
        ]
    }
}
